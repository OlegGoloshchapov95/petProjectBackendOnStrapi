{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "Todo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "due": {
      "type": "date"
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "minor",
        "regular",
        "critical"
      ],
      "default": "regular",
      "required": true
    },
    "done": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "user": {
      "type": "json",
      "required": true
    },
    "items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::item.item",
      "mappedBy": "todo"
    }
  }
}
